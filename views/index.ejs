<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title></title>
    <link rel="stylesheet" href="style.css">
  </head>
  <body>
    <section class="container">
      <section class="vendor">
        <section class="indexItems">
          <h3>Items</h3>
          <ul class="options">
          <% for(var i=0; i<items.length; i++) {%>
            <li class="specs">
              <p><span>Name:</span><%= items[i].name %></p>
              <p><span>Price:</span>$<%= items[i].price %></p>
              <img src="<%= items[i].pic %>" alt="item image">
              <p><span>Stock:</span><%= items[i].stock %></p>
              <p><span>Code:</span><%= items[i].code %></p>
            </li>
          <% } %>
          </ul>
        </section>

    <section class="keypad" >
      <input id="vendNum" type="text" name="code" value="" placeholder="Code of Item you want">
      <button id="clickMe" type="button" name="button">Submit Choice</button>
    </section>

      <section class="display" >
        <img src="" alt="item image">
      </section>
    </section>

  </section>
  <script src="main.js" charset="utf-8"></script>
  </body>
</html>
